{
  "name": "@fingerprintjs/fingerprintjs-pro-svelte",
  "version": "0.0.1",
  "description": "FingerprintJS Pro integration for Svelte",
  "author": "Fingerprint, Inc (https://fingerprint.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/fingerprintjs-pro-svelte.git"
  },
  "scripts": {
    "generate-dts": "npx svelte-types-writer --no-js --override ./src/**/*.svelte && prettier src/providers/*.d.ts --write",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "build-all": "yarn build && yarn --cwd examples/svelte-kit build",
    "lint": "eslint --ext .js,.ts,.svelte --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:dts": "tsc --noEmit --isolatedModules dist/index.d.ts",
    "svelte:check": "svelte-check src && yarn --cwd examples/svelte-kit check"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "svelte": "src/index.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.48.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.7.0",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.75.6",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-eslint-parser": "^0.16.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs-pro-spa": "^0.4.0"
  }
}
